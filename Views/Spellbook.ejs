<html><head>
<title>Spellbook</title>
<script>

function toggleCleric(status){
	if(status == 0){
		document.getElementById('ClericSpells').style.visibility = 'visible';
		document.getElementById('ClericDomains').style.visibility = 'hidden';
	}else if(status == 1){
		document.getElementById('ClericSpells').style.visibility = 'hidden';
		document.getElementById('ClericDomains').style.visibility = 'visible';
	}
}
	
function showSpellSelection(screen){
	document.getElementById('BardSpellList').style.visibility = "hidden"
	document.getElementById('ClericSpellList').style.visibility = "hidden"
	document.getElementById('DruidSpellList').style.visibility = "hidden"
	document.getElementById('PaladinSpellList').style.visibility = "hidden"
	document.getElementById('RangerSpellList').style.visibility = "hidden"
	document.getElementById('SorcererSpellList').style.visibility = "hidden"
	document.getElementById('WarlockSpellList').style.visibility = "hidden"
	document.getElementById('WizardSpellList').style.visibility = "hidden"
	document.getElementById('AllSpellList').style.visibility = "hidden"
	document.getElementById(screen).style.visibility = "visible"
}
</script>

