	document.getElementById('WarlockSpellList').style.visibility = "hidden"
	document.getElementById('WizardSpellList').style.visibility = "hidden"
	document.getElementById('AllSpellList').style.visibility = "hidden"
	document.getElementById(screen).style.visibility = "visible"
}
</script>

